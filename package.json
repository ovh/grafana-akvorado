{
  "name": "akvorado",
  "version": "1.0.25",
  "description": "Akvorado netflow data source using akvorado api",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "licenses": "node generate-licenses.js",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "playwright test",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "prepare": "lezer-generator --typeScript src/codemirror/lang-filter/syntax.grammar -o src/codemirror/lang-filter/parser.ts"
  },
  "author": "Ovh",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@cyclonedx/webpack-plugin": "^3.15.0",
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-e2e": "1.2.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@playwright/test": "^1.41.2",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react-router-dom": "^5.2.0",
    "@types/testing-library__jest-dom": "5.14.8",
    "axios": "^1.7.7",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "license-checker": "^25.0.1",
    "prettier": "2.8.8",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-hook-plugin": "^1.0.7",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.17.0",
    "@codemirror/state": "^6.4.1",
    "@emotion/css": "11.10.6",
    "@grafana/data": "10.3.3",
    "@grafana/runtime": "10.3.3",
    "@grafana/schema": "10.3.3",
    "@grafana/ui": "10.3.3",
    "@lezer/generator": "^1.7.1",
    "@lezer/lr": "^1.0.0",
    "@uiw/react-codemirror": "^4.23.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.3"
  },
  "packageManager": "npm@10.2.4"
}
